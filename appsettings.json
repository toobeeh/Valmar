{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Valmar": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:9090",
        "Protocols": "Http1"
      },
      "gRPC": {
        "Url": "http://localhost:8080",
        "Protocols": "Http2"
      }
    }
  },
  "ConnectionStrings": {
    "Palantir": "server=DB_DOMAIN_NAME_SED_PLACEHOLDER;user id=palantir;database=palantir"
  },
  "DropChunk" : {
    "ChunkSize" : 500,
    "BranchingCoefficient" : 2
  },
  "BubbleChunk" : {
    "ChunkSize" : 100,
    "BranchingCoefficient" : 2
  }
}
