{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "tobeh.Valmar.Server": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Grpc": {
    "HostPort": 8080
  },
  "Prometheus": {
    "HostPort": 9090
  },
  "ConnectionStrings": {
    "Palantir": "server=DB_DOMAIN_NAME_SED_PLACEHOLDER;user id=palantir;database=palantir"
  },
  "DropChunk" : {
    "ChunkSize" : 500,
    "BranchingCoefficient" : 2
  },
  "BubbleChunk" : {
    "ChunkSize" : 100,
    "BranchingCoefficient" : 2
  }
}
