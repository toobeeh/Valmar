{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "tobeh.Valmar.Server": "Information"
    }
  },
  "AllowedHosts": "*",
  "Grpc": {
    "HostPort": 8080
  },
  "Prometheus": {
    "HostPort": 9090
  },
  "ConnectionStrings": {
    "Palantir": "server=db-host;user id=palantir;database=palantir"
  },
  "DropChunk" : {
    "ChunkSize": 2000,
    "BranchingCoefficient" : 2
  },
  "BubbleChunk" : {
    "ChunkSize": 200,
    "BranchingCoefficient" : 2
  }
}
